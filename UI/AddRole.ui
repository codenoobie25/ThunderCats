  <property name="styleSheet">
   <string notr="true">
QDialog {
    background-color: #1A0033;
    border: 2px solid #FFD500;
    border-radius: 10px;
}


QLabel {
    color: #FFD500;  
    font-size: 14px;
    font-weight: bold;
}

QLineEdit, QComboBox {
    background-color: #FFFFFF;  
    color: #2A004E;           
    border: 2px solid #520099; 
    padding: 6px;               
    border-radius: 6px;    
    font-size: 14px;
}


QLineEdit:focus, QComboBox:focus {
    border: 2px solid #FFD500; 
    background-color: #FFFDF0;  
}

QPushButton#Addbutton {
    background-color: #FFD500;  
    color: #2A004E;        
    border-radius: 6px;
    font-weight: bold;
    font-size: 14px;
    border: none;
    padding: 8px 16px;
}

QPushButton#Addbutton:hover {
    background-color: #FFEA80; 
}

QPushButton#Cancelbutton {
    background-color: transparent;
    color: #FFFFFF;         
    border: 2px solid #520099; 
    border-radius: 6px;
    font-weight: bold;
    font-size: 14px;
    padding: 8px 16px;
}

QPushButton#Cancelbutton:hover {
    background-color: #520099; 
    color: #FFFFFF;
}</string>
